* {
  box-sizing: border-box;
}
.btnP {
  border: none;
  background: linear-gradient(to bottom, #4d4d4d, #808080);
  border-radius: 3px;
  font-family: Arial;
  color: #ffffff;
  padding: 5px 10px 5px 10px;
  text-decoration: none;
  margin: 5px;
  min-width:100px;
}

.btnP:hover {
  background: linear-gradient(to bottom,#4d4d4d, #808080);
  text-decoration: none;
  background-image:linear-gradient(to bottom,#4d4d4d, #808080);
  opacity:0.7;
}

.btnP.active:hover {
  background: linear-gradient(to bottom,#b32400, #b32400);
  text-decoration: none;
  background-image:linear-gradient(to bottom,#b32400, #b32400);
  opacity:0.7;
}

.btnP.active {
  background: linear-gradient(to bottom,#661400, #b32400);
  text-decoration: none;
  background-image:linear-gradient(to bottom,#661400, #b32400);
}

