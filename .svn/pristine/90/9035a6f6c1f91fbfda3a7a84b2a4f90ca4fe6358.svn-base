.post-it-note {
   background: #eeeeee;
   position: relative;
}

@media (max-width: 767px) {
    .post-it-note:after {
       content: "";
       position: absolute;
       bottom: -2.9em;
       left: -15px;
       right: 1.9em;
       border-width: 1.5em;
       border-style: solid;
       border-color: #eeeeee;
       margin-top:-5px;
    }
    .post-it-note:before {
       content: "";
       position: absolute;
       bottom: -2.9em;
       right: -15px;
       border-width: 3em 3em 0 0;
       border-style: solid;
       border-color: #d9d9d9 transparent;
      -webkit-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      -moz-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
    }
}

@media (min-width: 768px) {
    .post-it-note:after {
       content: "";
       position: absolute;
       bottom: -5.9em;
       left: -15px;
       right: 4.9em;
       border-width: 3em;
       border-style: solid;
       border-color: #eeeeee;
       margin-top:-5px;
    }
    .post-it-note:before {
       content: "";
       position: absolute;
       bottom: -5.9em;
       right: -15px;
       border-width: 6em 6em 0 0;
       border-style: solid;
       border-color: #d9d9d9 transparent;
      -webkit-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      -moz-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
    }
}
@media (min-width: 990px) {
    .post-it-note:after {
       content: "";
       position: absolute;
       bottom: -5.9em;
       left: -15px;
       right: 4.9em;
       border-width: 3em;
       border-style: solid;
       border-color: #eeeeee;
       margin-top:-5px;
    }
    .post-it-note:before {
       content: "";
       position: absolute;
       bottom: -5.9em;
       right: -15px;
       border-width: 6em 6em 0 0;
       border-style: solid;
       border-color: #d9d9d9 transparent;
      -webkit-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      -moz-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
    }
}
@media (min-width: 1200px) {
    .post-it-note:after {
       content: "";
       position: absolute;
       bottom: -5.9em;
       left: -15px;
       right: 4.9em;
       border-width: 3em;
       border-style: solid;
       border-color: #eeeeee;
       margin-top:-5px;
    }
    .post-it-note:before {
       content: "";
       position: absolute;
       bottom: -5.9em;
       right: -15px;
       border-width: 6em 6em 0 0;
       border-style: solid;
       border-color: #d9d9d9 transparent;
       z-index:1000;
      -webkit-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      -moz-box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
      box-shadow: -4px -4px 5px rgba(0,0,0,0.3), -4px -4px 4px rgba(0,0,0,0.2);
    }
}

